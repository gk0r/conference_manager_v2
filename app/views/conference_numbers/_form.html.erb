<%= simple_form_for(@conference_number, :html => {:class => 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :conference_number %>
  </div>

  <div class="form-actions">
		<%= f.button :submit, :class => 'btn btn-success' %>
		<%= link_to 'Cancel', conference_numbers_path, :class => 'btn btn-danger' %>
  </div>
<% end %>
